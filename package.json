{
  "name": "ngx-popper",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn build:lib && ng serve",
    "build": "yarn build:lib && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "test:schematics": "yarn build:build:schematics && jasmine schematics/**/*_spec.js",
    "build:schematics": "tsc -p schematics/tsconfig.json",
    "schematics": "yarn test:schematics && build:schematics",
    "lint:lib": "ng lint angular-popper",
    "test:lib": "ng test angular-popper",
    "build:lib": "ng build angular-popper --prod",
    "copy:readme": "node scripts/copy-readme.js",
    "copy:schematics": "copyfiles -u 2 \"./schematics/src/**/*.{js,json}\" dist/angular-popper/schematics",
    "publish:lib": "yarn lint:lib && yarn test:lib && yarn build:lib && yarn build:schematics && yarn copy:schematics && yarn copy:readme && npm publish dist/angular-popper",
    "build:demo": "ng build --prod --base-href /angular-popper/",
    "deploy:demo": "gh-pages -d dist/ngx-popper"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/router": "^7.2.9",
    "core-js": "^2.6.5",
    "popper.js": "^1.14.7",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular-devkit/build-ng-packagr": "~0.13.6",
    "@angular-devkit/core": "^7.3.6",
    "@angular-devkit/schematics": "^7.3.6",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "^7.2.9",
    "@types/jasmine": "~3.3.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.11.3",
    "codelyzer": "~5.0.0-beta.1",
    "copyfiles": "^2.1.0",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.3",
    "tsickle": ">=0.34.3",
    "tslib": "^1.9.3",
    "tslint": "~5.14.0",
    "typescript": "3.1.1"
  }
}
